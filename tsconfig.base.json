{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": [
      "ES2022",
      "DOM"
    ],
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@starter/db/*": [
        "packages/db/*"
      ],
      "@starter/shared/*": [
        "packages/shared/*"
      ]
    },
    "resolveJsonModule": true,
    "noEmit": true
  }
}
